<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.aky100200.mybatisdemo.mapper.UserInfoMapper">
    <select id="selectAll" resultType="com.aky100200.mybatisdemo.entity.UserInfo">
        SELECT * FROM userinfo;
    </select>
    <select id="selectId" resultType="com.aky100200.mybatisdemo.entity.UserInfo">
        SELECT * FROM userinfo where id = #{id};
    </select>
</mapper>
